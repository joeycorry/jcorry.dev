{
    "name": "jcorry.dev",
    "version": "1.0.0",
    "private": true,
    "author": {
        "email": "jcorry.git@gmail.com",
        "name": "Joey Corry",
        "url": "https://jcorry.dev"
    },
    "description": "A personal website about Joey Corry",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon --config ./config/nodemon.json",
        "format": "npm run format:write",
        "format:check": "prettier --config ./config/.prettierrc --cache --cache-location ./config/.prettier-cache . --check",
        "format:write": "prettier --config ./config/.prettierrc --cache --cache-location ./config/.prettier-cache . --write",
        "lint": "eslint --config ./config/.eslintrc --cache --cache-location ./config/.eslintcache --ext .ts,.tsx .",
        "prod": "npm run prod:build && npm run prod:serve",
        "prod:build": "rm -rf dist/ && npm run typecheck && npm run prod:build:client && npm run prod:build:server",
        "prod:build:server": "cross-env NODE_ENV=production esbuild --bundle --minify --splitting --external:./node_modules/* --format=esm --outdir=./dist --platform=node --target=$(browserslist-to-esbuild | sed \"s/ /,/g\") --tsconfig=./config/tsconfig.json ./src/server/index.ts",
        "prod:build:client": "cross-env NODE_ENV=production vite --config ./config/vite.config.ts build",
        "prod:serve": "cross-env NODE_ENV=production node ./dist/index.js",
        "typecheck": "tsc --project ./config/tsconfig.json --noEmit"
    },
    "dependencies": {
        "@vitejs/plugin-react": "4.2.1",
        "bezier-easing": "2.1.0",
        "cross-env": "7.0.3",
        "esbuild": "0.19.9",
        "h3": "1.9.0",
        "h3-compression": "0.3.2",
        "http-proxy": "1.18.1",
        "jotai": "2.6.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-feather": "2.0.10",
        "sirv": "2.0.3",
        "terser": "5.26.0",
        "tsx": "4.6.2",
        "typescript": "5.3.3",
        "vike": "0.4.150",
        "vite": "5.0.10"
    },
    "devDependencies": {
        "@types/compression": "1.7.5",
        "@types/http-proxy": "1.17.15",
        "@types/node": "20.10.5",
        "@types/react": "18.2.45",
        "@types/react-dom": "18.2.18",
        "@typescript-eslint/eslint-plugin": "6.14.0",
        "@typescript-eslint/parser": "6.14.0",
        "browserslist": "4.24.2",
        "browserslist-to-esbuild": "2.1.1",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-no-relative-import-paths": "1.5.3",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-unused-imports": "3.0.0",
        "nodemon": "3.0.2",
        "prettier": "3.1.1"
    },
    "browserslist": [
        "defaults"
    ],
    "type": "module"
}
