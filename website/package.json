{
    "name": "jcorry.dev",
    "version": "1.0.0",
    "private": true,
    "author": {
        "email": "jcorry.git@gmail.com",
        "name": "Joey Corry",
        "url": "https://jcorry.dev"
    },
    "description": "A personal website for Joey Corry",
    "scripts": {
        "build": "rm -rf dist/ && npm run typecheck && vite build && npm run build:server",
        "build:server": "esbuild --minify ./server/index.ts --outfile=dist/server.js",
        "serve": "cross-env NODE_ENV=production node --experimental-network-imports ./dist/server.js",
        "dev": "nodemon ./server/index.ts",
        "format": "prettier . --write --ignore-path .gitignore",
        "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
        "test:unit": "jest --watch",
        "test:unit:ci": "jest",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@vitejs/plugin-react": "3.0.1",
        "compression": "1.7.4",
        "cross-env": "7.0.3",
        "esbuild": "0.16.16",
        "express": "4.18.2",
        "jotai": "1.12.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-feather": "2.0.10",
        "sirv": "2.0.2",
        "terser": "5.16.1",
        "ts-node": "10.9.1",
        "typescript": "4.9.4",
        "vite": "4.0.4",
        "vite-plugin-ssr": "0.4.69"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/compression": "1.7.2",
        "@types/express": "4.17.15",
        "@types/jest": "29.2.5",
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@types/testing-library__user-event": "4.2.0",
        "@typescript-eslint/eslint-plugin": "5.48.0",
        "@typescript-eslint/parser": "5.48.0",
        "eslint": "8.31.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-import-resolver-typescript": "3.5.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-jest-dom": "4.0.3",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "8.0.0",
        "eslint-plugin-testing-library": "5.9.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "29.3.1",
        "jest-environment-jsdom": "29.3.1",
        "nodemon": "2.0.20",
        "prettier": "2.8.2",
        "ts-jest": "29.0.3"
    },
    "browserslist": [
        "defaults",
        "not IE 11",
        "not op_mini all"
    ],
    "type": "module"
}
