{
    "name": "jcorry.dev",
    "version": "1.0.0",
    "private": true,
    "author": {
        "email": "jcorry.git@gmail.com",
        "name": "Joey Corry",
        "url": "https://jcorry.dev"
    },
    "description": "A personal website for Joey Corry",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "scripts": {
        "dev": "nodemon",
        "format": "npm run format:write",
        "format:check": "prettier . --check",
        "format:write": "prettier . --write",
        "lint": "eslint --cache --ext .ts,.tsx .",
        "prod": "npm run prod:build && npm run prod:serve",
        "prod:build": "rm -rf dist/ && npm run typecheck && vite build && npm run prod:build:server",
        "prod:build:server": "esbuild --minify --target=es2022 ./server/index.ts --outfile=./dist/server.js",
        "prod:serve": "cross-env NODE_ENV=production node --experimental-network-imports ./dist/server.js",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@vitejs/plugin-react": "3.0.1",
        "compression": "1.7.4",
        "cross-env": "7.0.3",
        "esbuild": "0.19.9",
        "express": "4.18.2",
        "jotai": "2.0.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-feather": "2.0.10",
        "sirv": "2.0.3",
        "terser": "5.26.0",
        "tsx": "4.6.2",
        "typescript": "4.9.4",
        "vike": "0.4.150",
        "vite": "4.2.1"
    },
    "devDependencies": {
        "@types/compression": "1.7.5",
        "@types/express": "4.17.21",
        "@types/node": "20.10.5",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "6.14.0",
        "@typescript-eslint/parser": "6.14.0",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-no-relative-import-paths": "1.5.3",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-unused-imports": "3.0.0",
        "nodemon": "3.0.2",
        "prettier": "3.1.1"
    },
    "browserslist": [
        "defaults",
        "not IE 11",
        "not op_mini all"
    ],
    "type": "module"
}
