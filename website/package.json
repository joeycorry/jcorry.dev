{
    "name": "jcorry.dev",
    "version": "1.0.0",
    "private": true,
    "author": {
        "email": "jcorry.git@gmail.com",
        "name": "Joey Corry",
        "url": "https://jcorry.dev"
    },
    "description": "A personal website for Joey Corry",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3",
        "yarn": ">=1.22.19"
    },
    "scripts": {
        "dev": "nodemon",
        "format": "npm run format:write",
        "format:check": "prettier . --check",
        "format:write": "prettier . --write",
        "lint": "eslint --cache --ext .ts,.tsx .",
        "prod": "npm run prod:build && npm run prod:serve",
        "prod:build": "rm -rf dist/ && npm run typecheck && vite build && npm run prod:build:server",
        "prod:build:server": "esbuild --minify ./server/index.ts --outfile=./dist/server.js",
        "prod:serve": "cross-env NODE_ENV=production node --experimental-network-imports ./dist/server.js",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@vitejs/plugin-react": "3.0.1",
        "compression": "1.7.4",
        "cross-env": "7.0.3",
        "esbuild": "0.16.16",
        "express": "4.18.2",
        "jotai": "2.0.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-feather": "2.0.10",
        "sirv": "2.0.2",
        "terser": "5.16.1",
        "ts-node": "10.9.1",
        "typescript": "4.9.4",
        "vite": "4.2.1",
        "vite-plugin-ssr": "0.4.111"
    },
    "devDependencies": {
        "@types/compression": "1.7.2",
        "@types/express": "4.17.15",
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "5.48.0",
        "@typescript-eslint/parser": "5.48.0",
        "eslint": "8.31.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-import-resolver-typescript": "3.5.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-no-relative-import-paths": "1.5.2",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "8.0.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "nodemon": "2.0.20",
        "prettier": "2.8.2"
    },
    "browserslist": [
        "defaults",
        "not IE 11",
        "not op_mini all"
    ],
    "type": "module"
}
